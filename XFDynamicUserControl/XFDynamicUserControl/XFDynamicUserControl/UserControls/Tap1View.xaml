<?xml version="1.0" encoding="UTF-8"?>
<ContentView
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="XFDynamicUserControl.UserControls.Tap1View">

    <ContentView.Content>
        <StackLayout
            >
            <Label Text="Tab 1 View" HorizontalOptions="Center" VerticalOptions="Center" />
            <Label Text="{Binding Tab1Title}" HorizontalOptions="Center" VerticalOptions="Center" />
            <Button Text="Hello" Command="{Binding HelloCommand}" />
        </StackLayout>
    </ContentView.Content>
</ContentView>
